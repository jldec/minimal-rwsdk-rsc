{
  "name": "minimal-rwsdk-rsc",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite dev --force",
    "build": "vite build",
    "preview": "vite build && vite preview",
    "ship": "vite build && wrangler deploy",
    "types": "wrangler types --include-runtime false --strict-vars false",
    "tail": "wrangler tail"
  },
  "dependencies": {
    "rwsdk": "0.1.9"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.0",
    "@cloudflare/workers-types": "^4.20250703.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "daisyui": "^5.0.43",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.1",
    "wrangler": "^4.23.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "7"
      }
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none"
  }
}
